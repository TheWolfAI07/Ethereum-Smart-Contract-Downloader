{
  "name": "ethereum-smart-contract-downloader",
  "version": "1.0.0",
  "description": "A tool for extracting smart contracts from the Ethereum blockchain",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-backend": "mvn spring-boot:run",
    "start-backend-jar": "java -jar target/SmartContractExtractor-1.0.jar",
    "build-backend": "mvn clean package",
    "start-api": "func start",
    "dev": "concurrently \"npm run start-backend\" \"npm start\"",
    "build-all": "npm run build-backend && npm run build",
    "start-prod": "npm run build-all && npm run start-backend-jar"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^0.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^7.6.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080"
}
